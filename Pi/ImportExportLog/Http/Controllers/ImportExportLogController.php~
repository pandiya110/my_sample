//      
    public function importStores(Request $request){
      $data         = $request->all();
//      echo "<pre>";print_r($data);exit;
//      $command      = new ImportStores($data);   
      return $this->run(new ImportStores($data),trans('Import::messages.success_uploadImport'), trans('Import::messages.failure_uploadImport'));
      //$responseImportStores = CommandFactory::getCommand($command);
      //dd($responseImportStores);
    }
          
}


